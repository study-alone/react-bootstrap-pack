{
	"name": "react-bootstrap-pack",
	"private": true,
	"scripts": {
		"build": "turbo build",
		"dev": "turbo dev",
		"lint": "turbo lint",
		"clean": "turbo clean",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"restart-ui": "pnpm --filter @repo/restart-ui",
		"restart-hooks": "pnpm --filter @repo/restart-hooks",
		"rtg": "pnpm --filter @repo/react-transition-group",
		"document": "pnpm --filter docs"
	},
	"devDependencies": {
		"@repo/prettier-config": "workspace:*",
		"eslint": "^9.15.0",
		"prettier": "^3.3.3",
		"turbo": "^2.3.3",
		"typescript": "5.5.4"
	},
	"engines": {
		"node": ">=18",
		"pnpm": ">=9"
	},
	"prettier": "@repo/prettier-config",
	"packageManager": "pnpm@10.8.0"
}
